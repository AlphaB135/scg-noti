#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/bank/Desktop/dev/scg-noti/node_modules/.pnpm/ts-jest@29.3.3_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_647fe84a06783a941011b39ee7ab8cf8/node_modules/ts-jest/node_modules:/home/bank/Desktop/dev/scg-noti/node_modules/.pnpm/ts-jest@29.3.3_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_647fe84a06783a941011b39ee7ab8cf8/node_modules:/home/bank/Desktop/dev/scg-noti/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/bank/Desktop/dev/scg-noti/node_modules/.pnpm/ts-jest@29.3.3_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_647fe84a06783a941011b39ee7ab8cf8/node_modules/ts-jest/node_modules:/home/bank/Desktop/dev/scg-noti/node_modules/.pnpm/ts-jest@29.3.3_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_647fe84a06783a941011b39ee7ab8cf8/node_modules:/home/bank/Desktop/dev/scg-noti/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
