#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/bank/Desktop/dev/scg-noti/node_modules/yamljs/bin/node_modules:/home/bank/Desktop/dev/scg-noti/node_modules/yamljs/node_modules:/home/bank/Desktop/dev/scg-noti/node_modules:/home/bank/Desktop/dev/node_modules:/home/bank/Desktop/node_modules:/home/bank/node_modules:/home/node_modules:/node_modules:/home/bank/Desktop/dev/scg-noti/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/bank/Desktop/dev/scg-noti/node_modules/yamljs/bin/node_modules:/home/bank/Desktop/dev/scg-noti/node_modules/yamljs/node_modules:/home/bank/Desktop/dev/scg-noti/node_modules:/home/bank/Desktop/dev/node_modules:/home/bank/Desktop/node_modules:/home/bank/node_modules:/home/node_modules:/node_modules:/home/bank/Desktop/dev/scg-noti/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../yamljs/bin/yaml2json" "$@"
else
  exec node  "$basedir/../yamljs/bin/yaml2json" "$@"
fi
